{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "run-p check:*",
    "check:format": "prettier --cache -c '**/*.{ts,json,yml,yaml,md}'",
    "check:lint": "eslint --cache --max-warnings 0 '**/*.ts'",
    "check:tsc": "tsc",
    "check:tsc:watch": "tsc --watch",
    "cli": "npm run start ./src/cli/main.ts",
    "fix": "run-s fix:format fix:lint check:tsc",
    "fix:format": "prettier --cache --write '**/*.{ts,json,yml,yaml,md}'",
    "fix:lint": "eslint --cache --max-warnings 0 --fix '**/*.{ts,tsx}'",
    "sort": "sort-package-json",
    "start": "node -r esbuild-register",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "update:pkgs": "ncu -u"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.692.0",
    "@aws-sdk/lib-dynamodb": "3.692.0",
    "aws-xray-sdk": "3.10.2",
    "dayjs": "1.11.13",
    "fs-extra": "11.2.0",
    "get-stream": "9.0.1",
    "inversify": "6.1.4",
    "lodash.chunk": "4.2.0",
    "lodash.orderby": "4.6.0",
    "lodash.sortby": "4.7.0",
    "source-map-support": "0.5.21",
    "undici": "6.21.0",
    "uuid": "11.0.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@classmethod/aws-util": "file:../shared/aws-util",
    "@types/aws-lambda": "8.10.145",
    "@types/fs-extra": "11.0.4",
    "@types/inquirer": "9.0.7",
    "@types/lodash.chunk": "4.2.9",
    "@types/lodash.orderby": "4.6.9",
    "@types/lodash.sortby": "4.7.9",
    "@types/source-map-support": "0.5.10",
    "@types/uuid": "10.0.0",
    "dotenv": "16.4.5",
    "inquirer": "12.1.0",
    "vitest": "2.1.5"
  }
}
